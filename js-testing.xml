<templateSet group="js-testing">
  <template name="it" value="it(&quot;$description$&quot;, function () {&#10;  $END$&#10;});" description="it(&quot;description&quot;, fn)" toReformat="true" toShortenFQNames="true">
    <variable name="description" expression="" defaultValue="&quot;works&quot;" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="ass" value="assert.$fn$($END$);" description="assert (chai)" toReformat="true" toShortenFQNames="true">
