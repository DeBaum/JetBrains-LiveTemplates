    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="mount" value="const $varName$ = mount(&lt;$END$ /&gt;); " description="mount(&lt;Component /&gt;)" toReformat="true" toShortenFQNames="true">
    <variable name="varName" expression="" defaultValue="&quot;wrapper&quot;" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
</templateSet>